<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" style="-fx-background-color: rgba(223, 255, 255, 0.5);" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MainPagesClientController">
    <children>
        <BorderPane layoutX="6.0" prefHeight="500.0" prefWidth="800.0">
            <top>
                <HBox prefHeight="73.0" prefWidth="800.0" BorderPane.alignment="CENTER">
                    <children>
                        <Button mnemonicParsing="false" onAction="#showView1" text="View 1">
                            <HBox.margin>
                                <Insets left="100.0" top="30.0" />
                            </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#showView2" text="View 2">
                            <HBox.margin>
                                <Insets left="200.0" top="30.0" />
                            </HBox.margin>
                        </Button>
                  <Button layoutX="110.0" layoutY="40.0" mnemonicParsing="false" onAction="#showView3" text="View 3">
                     <HBox.margin>
                        <Insets left="200.0" top="30.0" />
                     </HBox.margin>
                  </Button>
                    </children>
                </HBox>
            </top>
         <center>
                <StackPane fx:id="contentArea" prefHeight="400.0" prefWidth="800.0" BorderPane.alignment="CENTER" />
         </center>
         <bottom>
            <HBox prefHeight="47.0" prefWidth="650.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="lbLogoutpageHome" mnemonicParsing="false" onAction="#CloseButton" prefHeight="31.0" prefWidth="80.0" style="-fx-background-color: rgba(97, 56, 255, 0.8);" text="LogOut" textFill="WHITE">
                     <HBox.margin>
                        <Insets left="710.0" />
                     </HBox.margin>
                     <font>
                        <Font name="Agency FB Bold" size="18.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </bottom>
        </BorderPane>
    </children>
</AnchorPane>
